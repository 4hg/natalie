def assert_equal(expected, actual)
  puts expected.to_s + " should be " + actual.to_s
end

def foo
  'foo'
end

def double(x)
  x * 2
end

def triple x
  x * 3
end

def mul1(x, y,
         z)
  x * y * z
end

def mul2 x, y, z
  x * y * z
end

assert_equal('foo', foo)
assert_equal(4, double(2))
assert_equal(9, triple 3)
assert_equal(24, mul1(2, 3, 4))
assert_equal(24, mul1 2, 3, 4)

outside_x = 1
def scope
  outside_x
end
assert_equal(1, scope)
outside_x = 2
assert_equal(2, scope)
